{
    "builds": [
      {
        "src": "mysite/mysite/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.10",
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "mysite/mysite/wsgi.py"
      }
    ]
  }
  